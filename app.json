{
  "expo": {
    "name": "CoffeeApp",
    "slug": "CoffeeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coffee-shop.png",
    "scheme": "Coffee",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coffee-shop.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/coffee-shop.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",{
          "fonts":[
            "assets/fonts/Inter-Black.otf",
            "assets/fonts/Inter-Bold.otf",
            "assets/fonts/Inter-ExtraLight.otf",
            "assets/fonts/Inter-ExtraBold.otf",
            "assets/fonts/Inter-Medium.otf",
            "assets/fonts/Inter-Thin.otf",
            "assets/fonts/Inter-Regular.otf",
            "assets/fonts/Inter-Semibold.otf",
            "assets/fonts/Inter-Light.otf",
            "assets/fonts/Inter-Italic.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
